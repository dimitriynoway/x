{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": "airbnb",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12
	},
	"plugins": [
		"react",
		"import"
	],
	"rules": {
		"indent": [
			2,
			"tab",
			{
				"SwitchCase": 1,
				"VariableDeclarator": 1
			}
		],
		"react/jsx-indent": [
			2,
			"tab"
		],
		"react/jsx-indent-props": [
			2,
			"tab"
		],
		"import/prefer-default-export": "off",
		"no-tabs": 0,
		"react/prop-types": 0,
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		]
	}
}