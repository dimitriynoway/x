{
	"compilerOptions": {
		"target": "es6",
		"lib": [
			"es6"
		],
		"module": "commonjs",
		"rootDir": "./src",
		"baseUrl": "src",
		"allowJs": true,
		"outDir": "./build",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		//"strict": true,
		"skipLibCheck": true,
		//from orm
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
	}
}